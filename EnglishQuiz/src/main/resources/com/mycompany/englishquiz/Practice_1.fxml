<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ChoiceBox fx:id="questionTypeChoiceBox" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
        <TextField fx:id="numQuestionsTextField" layoutX="170.0" layoutY="14.0" prefWidth="50.0" />
        <Button fx:id="startButton" layoutX="230.0" layoutY="14.0" text="Start" />
        <Label fx:id="questionLabel" layoutX="14.0" layoutY="50.0" prefWidth="572.0" />
        <VBox fx:id="answerOptionsVBox" layoutX="14.0" layoutY="100.0" prefWidth="572.0" />
        <TextField fx:id="answerTextField" layoutX="14.0" layoutY="300.0" prefWidth="572.0" />
        <Button fx:id="submitAnswerButton" layoutX="14.0" layoutY="330.0" text="Submit" />
        <Label fx:id="resultLabel" layoutX="100.0" layoutY="330.0" prefWidth="486.0" />
        <TableView fx:id="questionTableView" layoutX="14.0" layoutY="100.0" prefHeight="200.0" prefWidth="572.0">
            <columns>
                <TableColumn fx:id="questionColumn" prefWidth="200.0" text="Question" />
                <TableColumn fx:id="answerColumn" prefWidth="200.0" text="Answer" />
                <TableColumn fx:id="resultColumn" prefWidth="172.0" text="Result" />
            </columns>
        </TableView>
    </children>
</AnchorPane>